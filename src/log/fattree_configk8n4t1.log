hold_switch_for_packet=1;

vc_buf_size = 16;


wait_for_tail_credit = 0;

//
// Router architecture
//
vc_allocator = separable_input_first; 
sw_allocator = separable_input_first;
alloc_iters  = 1;

credit_delay   = 2;
routing_delay  = 0;
vc_alloc_delay = 1;
sw_alloc_delay = 1;
st_final_delay = 1;

input_speedup     = 1;
output_speedup    = 1;
internal_speedup  = 1.0;


warmup_periods = 3;
sim_count          = 1;

sample_period  = 10000;  


num_vcs     = 4;

priority = none;
// uniform / bitcomp
traffic       = uniform;   //or tornado

// 50% flit injection rate
injection_rate = 0.5;
packet_size = 1;
injection_rate_uses_flits=1;


// rounting function calling fattree_nca
topology = fattree;
routing_function = nca;


// t: bandwidth tappering 
k  = 8;
n  = 4;
t  = 1;     

watch_out=-;


// output file
stats_out = stats_out/fattree_uni_t1.m;;END Configuration File: config/fattree_nca
