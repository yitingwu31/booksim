// Dragonfly
//




vc_buf_size = 256;


wait_for_tail_credit = 0;

//
// Router architecture
//
vc_allocator = separable_input_first; 
sw_allocator = separable_input_first;
alloc_iters  = 1;

credit_delay   = 2;
routing_delay  = 0;
vc_alloc_delay = 1;
sw_alloc_delay = 1;
st_final_delay = 1;

input_speedup     = 1;
output_speedup    = 1;
internal_speedup  = 2.0;


warmup_periods = 3;
sim_count          = 1;

sample_period  = 10000;  



routing_function = min;
num_vcs     = 2;

priority = none;
traffic       = uniform; 

// 1% flit injection rate
injection_rate = 0.01;      
packet_size = 10;
injection_rate_uses_flits=1;

topology = dragonflynew;

// LIMITATION
  //  -- only one dimension between the group
  // _n == # of dimensions within a group (restricted to 1)
  // _k == # of processors within a router = the radix of each switch
  // inter-group ports : _k
  // terminal ports : _k
  // intra-group ports : 2*_k - 1

k  = 4;
n  = 1;

watch_out=-;

// output file
stats_out = stats_out/dragonfly_fin1.m;


END Configuration File: config/dragonfly_fin1
